    <div id="phoneFloat">
<div id="container">
          <% allHouses.forEach(function(house) { %>
          <div class="col-lg-3 col-md-4 col-sm-6 well">
      <a href="<%= house._id %>/edit">Edit</a>
      <div class="statusButtons"><button id="new" disabled="true">MLS Listed</button><button id="new2" disabled="true">Status</button><button id="new2" disabled="true">New!</button></div>
      <div class="listingImage"><a href="<%= house._id %>/show2"><img class="image" src="/images/house.jpg" height="200px" width="250px"></a></div>
      <div class="prevNextFav"><button>Prev</button><button>Next</button><button><img src="images/favorite-icon.png" height="15px" width="15px"> Favorite</button></div>
      <div class="listingBasics" class="text-center"><div><strong><%= house.price %></strong>&nbsp;&nbsp;&nbsp;&nbsp;[#]Days on Market</div>
      <div><%= house.bedrooms %> Bed, <%= house.bathrooms %> Bath, <%= house.squareFeet %> Sq.Ft.</div>
      <div>Neighborhood: <%= house.subdivision %></div>
      <div>[listing brokerage placeholder]</div><br></div>
      <div class="amenities"><% if (house.pool === 'yes') { %>
    <button id="none" disabled="true"><img src="images/icon-swimming.png" height="20px" width="20px"> Pool</button><% } %><% if (house.masterMain === 'yes') { %><button id="none" disabled="true"><img src="images/master-main-icon.ico" height="20px" width="20px"> Master on Main</button><% } %>
      <br><% if (house.greenbelt === 'yes') { %><button id="none" disabled="true"><img src="images/trees-icon.png" height="20px" width="20px"> Greenbelt</button><% } %><% if (house.golfCommunity === 'yes') { %><button id="none" disabled="true"><img src="images/golf-icon2.png" height="20px" width="20px"> Golf Community</button><% } %><% if (house.study === 'yes') { %><button id="none" disabled="true"><img src="images/study-icon.png" height="20px" width="20px"> Separate Study</button><% } %><% if (house.mediaRoom === 'yes') { %><button id="none" disabled="true"><img src="images/media-room-icon.png" height="20px" width="20px"> Media Room</button><% } %>
      <br><% if (house.fireplace === 'yes') { %><button id="none" disabled="true"><img src="images/fireplace-icon.png" height="20px" width="20px"> Fireplace</button><% } %><% if (house.communityPool === 'yes') { %><button id="none" disabled="true"><img src="images/swimming.png" height="20px" width="20px"> Community Pool</button><% } %><% if (house.waterfront === 'yes') { %><button id="none" disabled="true"><img src="images/waterfront-icon.png" height="20px" width="20px"> Waterfront</button><% } %><% if (house.lake === 'yes') { %><button id="none" disabled="true"><img src="images/waterfront-icon.png" height="20px" width="20px"> Community Lake</button><% } %></div>

  <!-- <div><button id="separateStudy"><img src="images/favorite-icon.png" height="20px" width="20px"> Add Favorite</button></div> -->
<br>
<br>
</div>
<% }) %>
